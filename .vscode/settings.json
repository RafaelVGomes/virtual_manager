{
  "files.associations": {
    "*.html": "jinja-html"
  },
  "workbench.iconTheme": "material-icon-theme",
  "emmet.includeLanguages": {
      "jinja-html": "html"
  },
  // "material-icon-theme.languages.associations": {
  //     "jinja-html": "html"
  // },
  "python.analysis.typeCheckingMode": "off",
  "python.analysis.autoImportCompletions": true,
  "cSpell.words": [
    "appcontext",
    "autofocus",
    "autohide",
    "camelcase",
    "DECLTYPES",
    "endfor",
    "endmacro",
    "endwith",
    "errorname",
    "est√°",
    "executescript",
    "jsonify",
    "lastrowid",
    "pyfile",
    "sqlite",
    "tbody",
    "tojson",
    "Verifica",
    "werkzeug"
  ],
  "cSpell.ignoreWords": [
    "dflt"
  ],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "files.exclude": {
    "**/__pycache__": true,
    ".venv": true
  }
}